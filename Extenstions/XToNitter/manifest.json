{
    "manifest_version": 2,
    "name": "Twitter to Nitter",
    "version": "1.0",
    "description": "Replace twitter with nitter",
  
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["content.js"]
      }
    ],
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]
  }
  